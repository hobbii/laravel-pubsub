{
  "name": "hobbii/laravel-pubsub",
  "description": "Pub/Sub for Laravel using AWS SQS and SNS",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "illuminate/support": "^8.81|^9.0",
    "aws/aws-sdk-php": "^3.209",
    "illuminate/broadcasting": "^8.81|^9.0",
    "illuminate/queue": "^8.81|^9.0",
    "illuminate/http": "^8.81|^9.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.4",
    "phpunit/phpunit": "^9.5",
    "fakerphp/faker": "^1.18",
    "orchestra/testbench": "^6.24",
    "friendsofphp/php-cs-fixer": "^3.8"
  },
  "autoload": {
    "psr-4": {
      "Hobbii\\PubSub\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hobbii\\PubSub\\PubSubServiceProvider"
      ]
    }
  }
}
